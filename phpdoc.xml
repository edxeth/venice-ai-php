<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
    xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd"
>
    <title>Venice PHP SDK</title>
    <paths>
        <output>docs/venice-ai-php</output>
        <cache>build/cache</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn=".">
                <path>VeniceAI.php</path>
                <path>examples</path>
            </source>
            <ignore>
                <path>vendor/**/*</path>
                <path>tests/**/*</path>
                <path>docs/**/*</path>
            </ignore>
            <visibility>public</visibility>
            <default-package-name>Venice</default-package-name>
        </api>
    </version>
    <template name="clean"/>
</phpdocumentor>